<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LISTANDO DADOS</title>
</head>
<body>

    <h1>LISTAGEM DE USUARIOS</h1>
    <p><a href="/add">Adicionar</a></p>

    <table>
        <tr>
            <th>ID</th>
            <th>NOME</th>
            <th>EMAIL</th>
            <th>SENHA</th>
            <th>FOTO</th>
            <th>DELETAR</th>
        </tr>
        <% Usuarios.forEach(function(usuario){ %>
        <tr>
            <td><%=usuario._id%></td>
            <td><%=usuario.nome%></td>
            <td><%=usuario.email%></td> 
            <td><%=usuario.senha%></td>
            <td><%=usuario.foto%></td>
            <td><a href="/del/<%=usuario._id%>">DELETAR</a></td>
        </tr>
        <% }) %>
    </table>
</body>
</html> 